<header>

    <a href="/" class="logo">audiophile</a>

    <nav>
        <ul>
            {% for link in section.settings.main_menu.links %}
                <li>
                    <a href="{{ link.url }}">{{ link.title }}</a>
                </li>
            {% endfor %}
        </ul>
    </nav>

</header>

{{ 'header.css' | asset_url | stylesheet_tag }}

{% schema %}
{
    "name": "Header",
    "settings": [
        {
            "label": "Main Menu",
            "id": "main_menu",
            "type": "link_list"
        }
    ],
    "presets": [
        {
            "name": "Header"
        }
    ]
}
{% endschema %}